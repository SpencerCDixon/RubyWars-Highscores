<div id="logo"><img src="title.png"> </div>

<h1 id="title"> Top 25 High Scores </h1>

<div class="wrapper">
  <div class="row">
    <div class="col-md-12">
      <table class="table">
        <tr>
          <th><strong>Name</strong></th>
          <th><strong>Score</strong></th>
        </tr>
        <% @scores.each do |score| %>
        <tr>
          <td> <%= score["name"] %> </td>
          <td> <%= score["score"] %> </td>
        </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
